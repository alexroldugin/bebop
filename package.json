{
  "name": "bebop",
  "version": "0.0.4",
  "main": "index.js",
  "repository": "https://github.com/kumabook/bebop",
  "author": "Hiroki Kumamoto <kumabook@live.jp>",
  "license": "MIT",
  "scripts": {
    "get-unbranded-firefox": "get-firefox -ecb unbranded-release",
    "build": "webpack && web-ext build --overwrite-dest -i src/ \"**/*~\"",
    "start": "webpack && web-ext run -i src/ \"**/*~\" --bc -f $FIREFOX_BINARY",
    "sign": "webpack && web-ext sign -i src/ \"**/*~\" --api-key $API_KEY --api-secret $API_SECRET",
    "pack": "git archive HEAD --output=bebop.zip",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "get-firefox": "^1.5.0"
  },
  "permissions": {
    "multiprocess": true
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "history": "^4.7.2",
    "kiroku": "^0.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-treeview": "^0.4.7",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-saga-router": "^2.1.1",
    "regenerator-runtime": "0.10.0",
    "regenerator-transform": "^0.11.0",
    "uuid": "^3.1.0",
    "web-ext": "^2.2.2",
    "webextension-polyfill": "^0.2.1",
    "webpack": "^3.8.1"
  },
  "hasEmbeddedWebExtension": true
}
